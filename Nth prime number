#include <stdio.h>
void prime(int);
int main()
{
   int n,p;
   scanf("%d",&n);
   prime(n);
   //printf(" : %d\n",p);
}
void prime(int n)
{
	int n1=1;
	int i=2,j=2,count=0;
	while(n1<=n)
	{
		j=2;
		count=0;
		while(j<=i/2)
		{
			if(i%j==0)
			{
				count=1;
				break;
			}
			j++;
		}
		if(count==0){if(n1==n) break;n1++;}
		i++;
	}
	printf("%d",i);
}
