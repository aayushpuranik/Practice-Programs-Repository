#include<stdio.h>
#include<string.h>
#include<assert.h>

char *string;
void swap(char *x,char *y)
{
	char temp;
	temp=*x;
	*x=*y;
	*y=temp;
}

void permute(char *a,int l,int r,char *dd)
{
	int i;
	if(l==r)
	{
		if(strcmp(a,dd)==0)
		{
			string="YES";
			return;
		}
	}
	else
	{
		for(i=l;i<=r;i++)
		{
			swap((a+l),(a+i));
			permute(a,l+1,r,dd);
			swap((a+l),(a+i)); //backtrack
		}
	}
}

int main()
{
	int i=0,j=0,lenb=0;
	char *cc, *dd;
	char a[100],b[100];
	scanf("%s",a);
	cc=a;
	scanf("%s",b);
	dd=b;
	while(*dd!='\0'){dd++;lenb++;}
	cc=a;
	dd=b;
	string="NO";
	permute(dd,0,lenb-1,cc);
	printf("%s",string);
	return 0;
}
